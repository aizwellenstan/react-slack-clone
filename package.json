  "scripts": {
    "start": "node ./dist/main.js",
    "test": "run-s test:*",
    "test:jest": "jest --coverage",
    "test:eslint": "eslint '{client,server}/**/*.{ts,tsx}'",
    "test:prettier": "prettier -c '{client,server}/**/*.{ts,tsx}'",
    "test:stylelint": "stylelint 'client/**/*.{tsx,tsx}'",
    "test:tsc": "tsc -p ./client",
    "build": "run-p build:*",
    "build:server": "tsc -p ./server",
    "build:client": "webpack --mode=production",
    "watch": "run-p watch:*",
    "watch:client": "webpack-dev-server --mode=production",
    "watch:server": "ts-node server/main.ts",
    "manage:translations": "i18next-scanner 'client/**/*.{ts,tsx}' --config=i18next-scanner.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "stylelint",
      "git add"
    ]
  },
